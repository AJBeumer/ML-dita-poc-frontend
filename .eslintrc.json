{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "plugin:prettier/recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
      "error", {
        "argsIgnorePattern": "next"
      }
    ]
  }
}
